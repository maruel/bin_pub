[Unit]
Description=Warmup claude via md

[Service]
Type=oneshot
WorkingDirectory=%h/bin/bin_pub
ExecStart=%h/src/md/md run /home/user/.local/bin/claude --model haiku -p 'Say hello, nothing else'
